{% block header %}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />

    <title>Project Manager</title>
    <link rel="stylesheet" type="text/css" href="/css/styles.css">
    <link type="text/css" rel="stylesheet" href="/tiitz/css/bootstrap.css">
    <link href='http://fonts.googleapis.com/css?family=Jockey+One' rel='stylesheet' type='text/css'>


    <link type="text/css" rel="stylesheet" href="/js/dataTable/media/css/demo_page.css">
    <link type="text/css" rel="stylesheet" href="/js/dataTable/media/css/jquery.dataTables_themeroller.css">
    <link type="text/css" rel="stylesheet" href="/js/dataTable/examples/examples_support/themes/smoothness/jquery-ui-1.8.4.custom.css">

    <script type="text/javascript" src="/tiitz/js/jquery-1.9.0.min.js"></script>

    <script type="text/javascript" language="javascript" src="/js/toolBar/toolBar.js" ></script>
    <script type="text/javascript" language="javascript" src="/js/toolBar/toolBarController.js" ></script>


    <style type="text/css" title="currentStyle">



    </style>
</head>

{% endblock %}
<body class="tiitz">
<header class="row-fluid header">
    <div class="span2 containr">
        <a href="#" class="logo"></a>
    </div>
    <div  id="topNav" class="span3 offset7">

        <a href="#">My Account</a>

        <a href="#">Projects</a>

        <a href="#">Activities</a>

        <a class="deco" href="#">Sign Out</a>
    </div>
</header>

{% include presentation with {'presentation' : presentation } %}

{% include toolBar with {'toolBar' : toolBarRules } %}


<div class="row-fluid" id="toolBar">
</div>
<div class="row-fluid" id="beginContent">
    <div  id="content" class="span10 offset1">
        <div  class="container-fluid">
            <div class="span8">
                {% block contentLeft %}

                {% endblock %}
            </div>
            <div class="span3 offset1">
                {% block contentRight %}

                {% endblock %}
            </div>
        </div>

    </div>
</div>

{% block footer %}

    <footer class="row-fluid footer">

    </footer>



{% endblock %}
</body>
</html>
